{
  "name": "common",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "common",
  "tags": ["go", "shared", "library"],
  "implicitDependencies": [],
  "targets": {
    "build": {
      "options": {
        "command": "go build ."
      }
    },
    "test": {
      "options": {
        "command": "go test -v ./..."
      }
    },
    "go-mod-tidy": {
      "options": {
        "command": "go mod tidy"
      }
    },
    "golangci-lint": {
      "options": {
        "command": "golangci-lint run --fix"
      }
    }
  }
}

